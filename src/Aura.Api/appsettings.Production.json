{
  "ConnectionStrings": {
    "auradb": "Host=localhost;Port=5433;Database=auradb;Username=postgres"
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://localhost:5300"
      }
    }
  },
  "Aura": {
    "Agents": {
      "Directories": ["../agents"],
      "EnableHotReload": false
    },
    "Prompts": {
      "Directories": ["../prompts"]
    },
    "Modules": {
      "Developer": {
        "LanguagesPath": "../agents/languages"
      }
    },
    "Llm": {
      "Providers": {
        "Ollama": {
          "NumGpu": -1,
          "MaxEmbeddingTextLength": 30000
        }
      }
    }
  }
}
